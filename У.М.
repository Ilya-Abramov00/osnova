clear
close
fid = fopen('D:\file1EuropaPlus.bin', 'r');
a = fread(fid, 'float32','l');
fclose(fid);

s = complex(a(1:2:end/2), a(2:2:end/2)); 

Fs =500*(10^3)
Ts = 1/Fs;
t = 0:Ts/(length(s)-1):Ts;
s=s.';

A = double(real(s));
B = double(imag(s));

e=25;

B1=diff(B);
A1=diff(A);
A1= [A1 0];
B1=[B1 0];

q1=B1.*A -A1.*B;
q2=A.^2 +B.^2;

w=q1./q2;

w=filter(filper,w);
 w= resample(w, 1, e);

sound(w, Fs/e)
